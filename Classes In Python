class Student:
    def __init__(self, shirtcolor, favoritedrink, favoritefood):
        self.favoritedrink = favoritedrink
        self.shirtcolor = shirtcolor
        self.favoritefood = favoritefood
        
    def Interests(self):
        return '{} {} {}'.format(self.shirtcolor, self.favoritedrink, self.favoritefood)

stud1 = Student('Red', 'Milk', 'Chicken')
stud2 = Student('Blue', 'Tea', 'Rice')
stud3 = Student('Green', 'Orange Juice', 'Jerky')
stud4 = Student('Purple', 'Apple Juice', 'Hamburger')


print(stud1.Interests())

print(Student.Interests(stud3))


